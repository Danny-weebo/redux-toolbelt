{
  "name": "redux-toolbelt",
  "version": "1.4.0",
  "description": "",
  "browser": "dist/redux-toolbelt.js",
  "main": "commonjs/index.js",
  "moudle": "lib/index.js",
  "scripts": {
    "not-avail-start": "concurrently --kill-others --prefix \"[{name}]\" --names \"WEBPACK,SERVER\" \"npm run build -- --watch\" \"live-server --no-browser --mount=/dist:./dist lib/examples\"",
    "build_orig": "cross-env NODE_ENV=build webpack --progress --env.build=production",
    "build": "babel lib --out-dir commonjs --plugins transform-es2015-modules-commonjs",
    "test:watch": "npm test -- --watch",
    "test": "cross-env NODE_ENV=test ava tests",
    "lint": "eslint lib && eslint tests",
    "lint:fix": "eslint --fix lib && eslint --fix tests",
    "prepublish": "npm run build"
  },
  "repository": "git+https://github.com/welldone-software/redux-toolbelt.git",
  "keywords": [],
  "author": "<danikenan@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/welldone-software/redux-toolbelt/issues"
  },
  "homepage": "https://github.com/welldone-software/redux-toolbelt#readme",
  "devDependencies": {
    "ava": "^0.18.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "concurrently": "^3.3.0",
    "cross-env": "^3.1.4",
    "eslint": "^3.15.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-ava": "^4.1.0",
    "live-server": "^1.2.0",
    "webpack": "^2.2.1",
    "webpack-merge": "^2.6.1"
  },
  "peerDependencies": {},
  "dependencies": {
    "empty": "^0.10.1",
    "lodash.inrange": "^3.3.6",
    "lodash.isarray": "^4.0.0",
    "lodash.isfunction": "^3.0.8",
    "lodash.ismatch": "^4.4.0",
    "lodash.without": "^4.4.0"
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-register"
    ]
  }
}
